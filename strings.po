msgctxt "STRINGS.SKIN_NAMES.wx78_survivor"
msgid "The Survivor"
msgstr "생존자"

#. STRINGS.SKIN_QUOTES.wathgrithr_combatant
msgctxt "STRINGS.SKIN_QUOTES.wathgrithr_combatant"
msgid "\"I yearn för my chance ön the pröving gröunds!\""
msgstr "\"나는 내 능력을 증명할 수 있는 기회를 갈망해 왔도다!\""

#. STRINGS.SKIN_QUOTES.wathgrithr_deerclops
msgctxt "STRINGS.SKIN_QUOTES.wathgrithr_deerclops"
msgid "\"I cöuld take yöu curs with öne eye behind my mask!\""
msgstr "\"나는 내 가면 뒤의 오직 한 눈 만으로도 그대를 박살낼 수 있노라!\""

#. STRINGS.SKIN_QUOTES.wathgrithr_formal
msgctxt "STRINGS.SKIN_QUOTES.wathgrithr_formal"
msgid "\"We will celebrate as if we were in Valhalla!\""
msgstr "우리 모두 발할라에 있는 듯이 축하할 것이다!\""

#. STRINGS.SKIN_QUOTES.wathgrithr_gladiator
msgctxt "STRINGS.SKIN_QUOTES.wathgrithr_gladiator"
msgid "\"My skill in battle is withöut peer.\""
msgstr "\"나의 전투 기술에는 따라올 자가 없구나.\""

#. STRINGS.SKIN_QUOTES.wathgrithr_shadow
msgctxt "STRINGS.SKIN_QUOTES.wathgrithr_shadow"
msgid "\"I will deströy my enemies!\""
msgstr "\"나의 적을 쳐부술 것이노라!\""

#. STRINGS.SKIN_QUOTES.wathgrithr_survivor
msgctxt "STRINGS.SKIN_QUOTES.wathgrithr_survivor"
msgid "\"Yöu have tö try harder than this tö defeat me!\""
msgstr "\"날 쓰러트리기 위해서는 그것보단 더 노력해야 할 것이다!\""

#. STRINGS.SKIN_QUOTES.waxwell_combatant
msgctxt "STRINGS.SKIN_QUOTES.waxwell_combatant"
msgid "\"I've faced worse ordeals.\""
msgstr "\"더한 시련도 겪어 봤지.\""

#. STRINGS.SKIN_QUOTES.waxwell_formal
msgctxt "STRINGS.SKIN_QUOTES.waxwell_formal"
msgid "\"I just need to find my beautiful assistant.\""
msgstr "\"난 단지 내 아름다운 조수를 찾길 원할 뿐이야.\""

#. STRINGS.SKIN_QUOTES.waxwell_gladiator
msgctxt "STRINGS.SKIN_QUOTES.waxwell_gladiator"
msgid "\"All those who oppose me shall perish!\""
msgstr "\"나를 반대하는 자는 누구든지 소멸할지니!\""

#. STRINGS.SKIN_QUOTES.waxwell_krampus
msgctxt "STRINGS.SKIN_QUOTES.waxwell_krampus"
msgid "\"Naughty. Definitely naughty.\""
msgstr "\"넌 말썽꾸러기가 분명하군.\""

#. STRINGS.SKIN_QUOTES.waxwell_survivor
msgctxt "STRINGS.SKIN_QUOTES.waxwell_survivor"
msgid "\"Say, pal, I don't look so good.\""
msgstr "\"이보게, 친구, 안 좋아 보이는군. 내가 말이야.\""

#. STRINGS.SKIN_QUOTES.waxwell_unshadow
msgctxt "STRINGS.SKIN_QUOTES.waxwell_unshadow"
msgid "\"The poor unsuspecting fool has no idea what's coming.\""
msgstr "\"이 불쌍하고 눈치없는 머저리는 무슨 일이 일어날지 까맣게 몰랐지.\""

#. STRINGS.SKIN_QUOTES.webber_bat
msgctxt "STRINGS.SKIN_QUOTES.webber_bat"
msgid "\"Raaar! ...That's our bat noise.\""
msgstr "\"으르렁! ...이거 우리 박쥐 소리야.\""

#. STRINGS.SKIN_QUOTES.webber_combatant
msgctxt "STRINGS.SKIN_QUOTES.webber_combatant"
msgid "\"We don't want to fight if we don't have to.\""
msgstr "\"우린 될 수 있으면 안 싸우고 싶어.\""

#. STRINGS.SKIN_QUOTES.webber_formal
msgctxt "STRINGS.SKIN_QUOTES.webber_formal"
msgid "\"We like parties!\""
msgstr "\"우린 파티 좋아해!\""

#. STRINGS.SKIN_QUOTES.webber_gladiator
msgctxt "STRINGS.SKIN_QUOTES.webber_gladiator"
msgid "\"If you fight us, you will lose.\""
msgstr "\"만약 니가 우리랑 싸운다면, 넌 질 거야.\""

#. STRINGS.SKIN_QUOTES.webber_shadow
msgctxt "STRINGS.SKIN_QUOTES.webber_shadow"
msgid "\"Come closer - we are hungry!\""
msgstr "\"가까이 와 - 우린 배고프니까!\""

#. STRINGS.SKIN_QUOTES.webber_survivor
msgctxt "STRINGS.SKIN_QUOTES.webber_survivor"
msgid "\"I want to go home, but we can't find the way.\""
msgstr "\"나 집에 가고 싶어, 근데 우리 길을 못 찾겠어.\""

#. STRINGS.SKIN_QUOTES.wendy_combatant
msgctxt "STRINGS.SKIN_QUOTES.wendy_combatant"
msgid "\"Abigail and I never fight.\""
msgstr "\"아비게일과 전 절대 다투지 않아요.\""

#. STRINGS.SKIN_QUOTES.wendy_formal
msgctxt "STRINGS.SKIN_QUOTES.wendy_formal"
msgid "\"I'll only go to the party if Abigail comes too.\""
msgstr "\"아비게일도 같이 가야만 파티에 갈 거에요.\""

#. STRINGS.SKIN_QUOTES.wendy_gladiator
msgctxt "STRINGS.SKIN_QUOTES.wendy_gladiator"
msgid "\"I take no pleasure in this.\""
msgstr "\"이런 일은 전혀 즐겁지 않아요.\""

#. STRINGS.SKIN_QUOTES.wendy_lureplant
msgctxt "STRINGS.SKIN_QUOTES.wendy_lureplant"
msgid "\"So alluring...\""
msgstr "\"아주 매혹적이죠...\""

#. STRINGS.SKIN_QUOTES.wendy_shadow
msgctxt "STRINGS.SKIN_QUOTES.wendy_shadow"
msgid "\"Shadows are as black as ink, or night, or nightmares.\""
msgstr "\"그림자는 검죠. 잉크처럼, 혹은 밤처럼, 혹은 악몽처럼.\""

#. STRINGS.SKIN_QUOTES.wendy_survivor
msgctxt "STRINGS.SKIN_QUOTES.wendy_survivor"
msgid "\"I carry on for Abigail's sake.\""
msgstr "\"저는 아비게일을 위해 계속 살아나갈 거예요.\""

#. STRINGS.SKIN_QUOTES.wickerbottom_combatant
msgctxt "STRINGS.SKIN_QUOTES.wickerbottom_combatant"
msgid "\"Combat is all about technique, dear.\""
msgstr "\"전투는 오직 기교에 관한 거란다, 얘야.\""

#. STRINGS.SKIN_QUOTES.wickerbottom_formal
msgctxt "STRINGS.SKIN_QUOTES.wickerbottom_formal"
msgid "\"Even I let my hair down occasionally.\""
msgstr "\"심지어 나조차 내 머리를 가끔씩 풀어놓는단다.\""

#. STRINGS.SKIN_QUOTES.wickerbottom_gladiator
msgctxt "STRINGS.SKIN_QUOTES.wickerbottom_gladiator"
msgid "\"Goodness gracious, it seems you've been annihilated.\""
msgstr "\"이런 세상에, 너희 완전히 박살난 것 같구나.\""

#. STRINGS.SKIN_QUOTES.wickerbottom_lightninggoat
msgctxt "STRINGS.SKIN_QUOTES.wickerbottom_lightninggoat"
msgid "\"All it takes is a spark of genius.\""
msgstr "\"필요한 건 오직 천재의 영감뿐이지.\""

#. STRINGS.SKIN_QUOTES.wickerbottom_shadow
msgctxt "STRINGS.SKIN_QUOTES.wickerbottom_shadow"
msgid "\"Knowledge is power!\""
msgstr "\"아는 것이 힘이란다!\""

#. STRINGS.SKIN_QUOTES.wickerbottom_survivor
msgctxt "STRINGS.SKIN_QUOTES.wickerbottom_survivor"
msgid "\"I hope I don't break my glasses.\""
msgstr "\"나 스스로 내 안경을 깨 먹지 않았으면 좋겠구나.\""

#. STRINGS.SKIN_QUOTES.willow_combatant
msgctxt "STRINGS.SKIN_QUOTES.willow_combatant"
msgid "\"I can handle myself.\""
msgstr "\"난 스스로를 다룰 줄 알아.\""

#. STRINGS.SKIN_QUOTES.willow_dragonfly
msgctxt "STRINGS.SKIN_QUOTES.willow_dragonfly"
msgid "\"Woah, I'm smokin'! (No, really!)\""
msgstr "\"우와, 나 후끈한걸! (아니, 진짜라니깐!)\""

#. STRINGS.SKIN_QUOTES.willow_formal
msgctxt "STRINGS.SKIN_QUOTES.willow_formal"
msgid "\"He died before getting to see these beautiful flames.\""
msgstr "\"그는 이  아름다운 화염을 보기도 전에 죽어 버렸어.\""

#. STRINGS.SKIN_QUOTES.willow_gladiator
msgctxt "STRINGS.SKIN_QUOTES.willow_gladiator"
msgid "\"Play with fire and you'll get burned.\""
msgstr "\"불장난 하면 숯검댕이가 될 거야.\""

#. STRINGS.SKIN_QUOTES.willow_shadow
msgctxt "STRINGS.SKIN_QUOTES.willow_shadow"
msgid "\"Shadows are just inverted flames.\""
msgstr "\"그림자는 반전된 불꽃일 뿐이지.\""

#. STRINGS.SKIN_QUOTES.willow_survivor
msgctxt "STRINGS.SKIN_QUOTES.willow_survivor"
msgid "\"Surviving just requires strength of will. (And fire.)\""
msgstr "\"살아남는 건 강한 의지만 있으면 돼. (불이랑.)\""

#. STRINGS.SKIN_QUOTES.wilson_combatant
msgctxt "STRINGS.SKIN_QUOTES.wilson_combatant"
msgid "\"This may prove to be my most dangerous experiment yet.\""
msgstr "\"이건 내 사상 최대로 위험한 실험이 될 거야.\""

#. STRINGS.SKIN_QUOTES.wilson_formal
msgctxt "STRINGS.SKIN_QUOTES.wilson_formal"
msgid "\"I hate parties.\""
msgstr "\"난 파티가 싫어.\""

#. STRINGS.SKIN_QUOTES.wilson_gladiator
msgctxt "STRINGS.SKIN_QUOTES.wilson_gladiator"
msgid "\"I've the brains AND the brawn.\""
msgstr "\"난 머리도 좋지만, 체력 또한 뛰어나지.\""

#. STRINGS.SKIN_QUOTES.wilson_pigguard
msgctxt "STRINGS.SKIN_QUOTES.wilson_pigguard"
msgid "\"I'm sure I had a scientific reason for this.\""
msgstr "\"내가 이걸 입었던 건 과학적인 의도에서 비롯됐다는 걸 확신해.\""

#. STRINGS.SKIN_QUOTES.wilson_shadow
msgctxt "STRINGS.SKIN_QUOTES.wilson_shadow"
msgid "\"Mwahahaha!\""
msgstr "\"뫄하하하!\""

#. STRINGS.SKIN_QUOTES.wilson_survivor
msgctxt "STRINGS.SKIN_QUOTES.wilson_survivor"
msgid "\"There is a scientific solution, here. I know it.\""
msgstr "\"과학적 해결책이 있을 거야, 분명. 난 그걸 알아.\""

#. STRINGS.SKIN_QUOTES.winona_combatant
msgctxt "STRINGS.SKIN_QUOTES.winona_combatant"
msgid "\"I can tough anything out.\""
msgstr "\"난 무엇이든지 견딜 수 있지.\""

#. STRINGS.SKIN_QUOTES.winona_formal
msgctxt "STRINGS.SKIN_QUOTES.winona_formal"
msgid "\"The life of the party has arrived!\""
msgstr "\"파티의 꽃이 여기 도착했어!\""

#. STRINGS.SKIN_QUOTES.winona_gladiator
msgctxt "STRINGS.SKIN_QUOTES.winona_gladiator"
msgid "\"You seem to think you got a chance. Let's fix that.\""
msgstr "\"넌 너한테 기회가 있다고 생각하는 모양이구나. 그걸 바로잡아 줄게.\""

#. STRINGS.SKIN_QUOTES.winona_grassgecko
msgctxt "STRINGS.SKIN_QUOTES.winona_grassgecko"
msgid "\"Heh heh, this thing's itchy.\""
msgstr "\"헤 헤, 이거 근질거리는데.\""

#. STRINGS.SKIN_QUOTES.winona_shadow
msgctxt "STRINGS.SKIN_QUOTES.winona_shadow"
msgid "\"You are but a cog in my machine.\""
msgstr "\"너는 한낱 내 기계 부속품에 불과할 뿐이지.\""

#. STRINGS.SKIN_QUOTES.winona_survivor
msgctxt "STRINGS.SKIN_QUOTES.winona_survivor"
msgid "\"I don't break down easy.\""
msgstr "\"난 그렇게 쉽게 무너지지 않아.\""

#. STRINGS.SKIN_QUOTES.wolfgang_combatant
msgctxt "STRINGS.SKIN_QUOTES.wolfgang_combatant"
msgid "\"Mighty Wolfgang cannot wait to step into ring!\""
msgstr "\"힘센 볼프강 경기장 빨리 올라가고 싶다!\""

#. STRINGS.SKIN_QUOTES.wolfgang_formal
msgctxt "STRINGS.SKIN_QUOTES.wolfgang_formal"
msgid "\"I used my special moustache wax.\""
msgstr "\"나 특별한 콧수염 왁스 썼다.\""

#. STRINGS.SKIN_QUOTES.wolfgang_gladiator
msgctxt "STRINGS.SKIN_QUOTES.wolfgang_gladiator"
msgid "\"You will kneel before might of Wolfgang!\""
msgstr "\"너 볼프강의 강력함 앞에 무릎 꿇게 될 거다!\""

#. STRINGS.SKIN_QUOTES.wolfgang_shadow
msgctxt "STRINGS.SKIN_QUOTES.wolfgang_shadow"
msgid "\"Do not challenge the Mighty Wolfgang!\""
msgstr "\"강력한 볼프강한테 도전하지 마라!\""

#. STRINGS.SKIN_QUOTES.wolfgang_survivor
msgctxt "STRINGS.SKIN_QUOTES.wolfgang_survivor"
msgid "\"Wolfgang is tough. Lots of muscle.\""
msgstr "\"볼프강은 거칠다. 근육 많다.\""

#. STRINGS.SKIN_QUOTES.wolfgang_walrus
msgctxt "STRINGS.SKIN_QUOTES.wolfgang_walrus"
msgid "\"Wolfgang is large moustache man! Ha ha!\""
msgstr "\"볼프강은 커다란 수염맨이다! 하하!\""

#. STRINGS.SKIN_QUOTES.woodie_combatant
msgctxt "STRINGS.SKIN_QUOTES.woodie_combatant"
msgid "\"I've been in scraps before, eh?\""
msgstr "\"난 잡동사니 속에서 살았던 적도 있어, 응?\""

#. STRINGS.SKIN_QUOTES.woodie_formal
msgctxt "STRINGS.SKIN_QUOTES.woodie_formal"
msgid "\"I clean up pretty good, eh?\""
msgstr "\"나 꽤 깨끗하게 다듬었지, 응?\""

#. STRINGS.SKIN_QUOTES.woodie_gladiator
msgctxt "STRINGS.SKIN_QUOTES.woodie_gladiator"
msgid "\"Don't start a tussle you can't finish.\""
msgstr "\"감당 못 할 시비는 안 거는 게 좋아.\""

#. STRINGS.SKIN_QUOTES.woodie_shadow
msgctxt "STRINGS.SKIN_QUOTES.woodie_shadow"
msgid "\"Forests are full of shadows, eh?\""
msgstr "\"숲은 그림자로 가득하지, 응?\""

#. STRINGS.SKIN_QUOTES.woodie_survivor
msgctxt "STRINGS.SKIN_QUOTES.woodie_survivor"
msgid "\"I could do with a haircut and a cup of tea, eh?\""
msgstr "\"머리도 좀 깎고 차 한잔 해야겠네, 응?\""

#. STRINGS.SKIN_QUOTES.woodie_treeguard
msgctxt "STRINGS.SKIN_QUOTES.woodie_treeguard"
msgid "\"To chop the tree, you must be the tree.\""
msgstr "\"나무를 베려면, 먼저 나무가 되어야 해.\""

#. STRINGS.SKIN_QUOTES.wx78_combatant
msgctxt "STRINGS.SKIN_QUOTES.wx78_combatant"
msgid "\"THIS UNIT IS BATTLE READY\""
msgstr "\"본 개체 전투 준비 완료\""

#. STRINGS.SKIN_QUOTES.wx78_formal
msgctxt "STRINGS.SKIN_QUOTES.wx78_formal"
msgid "\"I POLISHED MY CHASSIS FOR THE OCCASION\""
msgstr "\"이 연회를 위해 섀시를 광냈다\""

#. STRINGS.SKIN_QUOTES.wx78_gladiator
msgctxt "STRINGS.SKIN_QUOTES.wx78_gladiator"
msgid "\"ALL FLESHLINGS WILL BE CRUSHED\""
msgstr "\"모든 살덩어리들은 박살날 것이다\""

#. STRINGS.SKIN_QUOTES.wx78_rhinorook
msgctxt "STRINGS.SKIN_QUOTES.wx78_rhinorook"
msgid "\"OVERCHARGED, READY TO CHARGE\""
msgstr "\"과충전됨, 돌진 준비 완료\""

#. STRINGS.SKIN_QUOTES.wx78_shadow
msgctxt "STRINGS.SKIN_QUOTES.wx78_shadow"
msgid "\"NOW THE OUTSIDE MATCHES THE INSIDE. EVIL.\""
msgstr "\"이제 내면과 외면이 일치함. 사악함\""

#. STRINGS.SKIN_QUOTES.wx78_survivor
msgctxt "STRINGS.SKIN_QUOTES.wx78_survivor"
msgid "\"RUSTED AND BUSTED\""
msgstr "\"녹슬고 고장남\""

